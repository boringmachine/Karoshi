<% @topics.each do |topic| %>
  <% unless topic.status %>
  <div class="topic_origin">
    <div class="topic_subject">ID<%= topic.id%>:<%= link_to topic.subject, topic %></div>
    <div class="topic_date"><%= Post.getDate(topic.id) %></div>
  	<div class="topic_body"><%= Post.getBody(topic.id) %></div>
  </div>
  <% end %>
<% end %>