<h1>Listing community_users</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Community</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @community_users.each do |community_user| %>
      <tr>
        <td><%= community_user.user_id %></td>
        <td><%= community_user.community_id %></td>
        <td><%= link_to 'Show', community_user %></td>
        <td><%= link_to 'Edit', edit_community_user_path(community_user) %></td>
        <td><%= link_to 'Destroy', community_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Community user', new_community_user_path %>
